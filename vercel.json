{
  "version": 2,
  "builds": [
    {
      "src": "tg-web-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/tg-web-app/dist/index.html"
    }
  ],
  "outputDirectory": "tg-web-app/dist"
}